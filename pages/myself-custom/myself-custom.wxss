.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
  background-color: #f2f2f2
}

page{
    background-color:#f5f5f5;
}

.myHeader {
    width: 100%;
    height: 230px;
    /*background: url("../images/background.png") repeat #35495D;*/
    display: flex;
    flex-direction: column;
    /*background-size:cover;*/
     background: url("http://5b0988e595225.cdn.sohucs.com/images/20200223/0df4538f7226464a98bc085f3c9d1025.jpeg") no-repeat #35495D fixed top;
}

.topSet {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: baseline
}

.portr {
    width: 70px;
    height: 70px;
    margin: 0 auto;
    margin-top: 20px;
    border-radius: 70px
}

.nickName {
    color: white;
    font-size: 16px;
    text-align: center;
    margin-top: 15px;
    float: left
}

.aixin {
    width: 20px;
    height: 20px;
    border-radius: 10px;
    margin-left: 130px;
    margin-top: 10px
}

.welfare_val {
    color: white;
    font-size: 12px;
    text-align: center;
    margin-top: -15px
}

.myLists {
    margin-top: 10px;
    background-color: #f2f2f2
}

.callImage {
    width: 22px;
    height: 19px;
    margin-left: 12px;
    margin-top: 7px;
    float: left
}

.settingLabel {
    color: white;
    text-align: right;
    font-size: 14px;
    margin-right: 12px;
    /*margin-top: -10px;*/
    float: right
}

.cell {
    width: 100%;
    height: 44px;
    background-color: white;
    line-height: 44px
}

.cellIndicator {
    width: 7px;
    height: 13px;
    float: right;
    margin-right: 12px;
    margin-top: 15.5px
}

.lineSpace {
    width: 100%;
    height: 1px;
    background-color: #e0e0e0;
    margin-top: -1px;
    float: left
}

.cellImage {
    width: 21px;
    height: 21px;
    margin-left: 12px;
    margin-top: 11.5px;
    float: left
}

.cellTitle {
    margin-left: 10px;
    font-size: 14px;
    color: #545454;
    float: left
}

.cellContent {
    text-align: right;
    margin-right: 12px;
    float: right;
    font-size: 14px;
    color: #999999
}


/*二级菜单外部容器样式*/
.menu dd{
     position: absolute;
     width: 20%;
     top:30px; 
     right:10px;
     z-index:999
}
/*二级菜单普通样式*/
.menu li{
   font-size: 14px; 
   line-height: 34px;
   color: #575757;
   height: 34px;
   display: block;
   padding-left: 8px;
   background-color: #fff;
   border-bottom: 1px solid #dbdbdb
}

.hide{
    display:none
}